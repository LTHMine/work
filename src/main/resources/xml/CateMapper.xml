<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lth.work.mapper.CateMapper">

    <select id="findAll" resultType="com.lth.work.pojo.Category">
        select * from sort;
    </select>
    <select id="findById" resultType="com.lth.work.pojo.Category">
        select * from sort where id =#{id};
    </select>
</mapper>